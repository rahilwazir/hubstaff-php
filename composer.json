{
  "name": "HookOps/hubstaff-php",
  "type": "library",
  "description": "A PHP toolkit for Hubstaff API",
  "homepage": "https://github.com/HookOps/hubstaff-php",
  "keywords": [
    "php",
    "hubstaff",
    "hookops"
  ],
  "require": {
    "php": "^5.6 || ^7.1",
    "ext-curl": "*",
    "beberlei/assert": "2.*",
    "symfony/yaml": "2.*|3.*",
    "symfony/event-dispatcher": "~2.1|3.*"
  },
  "autoload": {
    "psr-4": {
      "Hubstaff\\": "src"
    },
    "classmap": [
      "config.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload-dev": {
    "psr-4": {
      "Hubstaff\\": "tests/"
    }
  }
}
